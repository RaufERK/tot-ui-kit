{
  "name": "tot-ui-kit",
  "version": "1.0.3",
  "description": "UI Kit with Layout, MainMenu, and theme support for React applications",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./global.css": "./src/global.css",
    "./styles": "./dist/index.css",
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "src/global.css"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --out-dir dist",
    "dev": "tsup src/index.ts --format esm,cjs --dts --out-dir dist --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "ui-kit",
    "layout",
    "menu",
    "theme"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@sberbusiness/icons-next": "^1.11.0",
    "@sberbusiness/triplex-next": "^1.14.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.22.0"
  },
  "peerDependencies": {
    "@sberbusiness/icons-next": "^1.11.0",
    "@sberbusiness/triplex-next": "^1.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
